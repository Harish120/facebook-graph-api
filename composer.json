{
    "name": "harryes/facebook-graph-api",
    "description": "A comprehensive Laravel package for Facebook Graph API with modern design patterns and excellent developer experience",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "facebook",
        "graph-api",
        "social-media",
        "api"
    ],
    "authors": [
        {
            "name": "Harryes",
            "email": "pantharish120@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/config": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6",
        "laravel/pint": "^1.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Harryes\\FacebookGraphApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Harryes\\FacebookGraphApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harryes\\FacebookGraphApi\\FacebookGraphApiServiceProvider"
            ],
            "aliases": {
                "FacebookGraph": "Harryes\\FacebookGraphApi\\Facades\\FacebookGraph"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 